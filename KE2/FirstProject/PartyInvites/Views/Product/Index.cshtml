
@model IEnumerable<Product>



<h1>Index</h1>

<table class="table table-bordered table-striped table-sm">
    <thead class="thead-dark    ">
        <tr>
            <th>Name</th>
            <th>Category</th>
            <th>Name</th>
            <th>Price</th>
            <th>In Stock</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
        <tr>
            <td>@r.Name</td>
            <td>@r.Category</td>
            <td>@r.Name</td>
            <td>@r.Price</td>
            <td>@r.InStock</td>
        </tr>
        }
    </tbody>
</table>


